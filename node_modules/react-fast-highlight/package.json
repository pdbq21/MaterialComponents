{
  "name": "react-fast-highlight",
  "version": "2.1.1",
  "description": "A fast react component wrapper for highlight.js",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib",
    "flow": "flow",
    "lint": "eslint src",
    "prepublish": "yarn run clean && yarn run build",
    "preversion": "yarn test",
    "test": "yarn run lint && yarn run flow && yarn run test-only",
    "test-only": "cross-env BABEL_ENV=test nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chili-labs/react-fast-highlight.git"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "react",
    "component",
    "highlight",
    "syntax",
    "highlightjs"
  ],
  "author": "Daniel Tschinder <daniel@tschinder.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chili-labs/react-fast-highlight/issues"
  },
  "homepage": "https://github.com/chili-labs/react-fast-highlight#readme",
  "peerDependencies": {
    "react": "^15.3.0"
  },
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "cross-env": "^4.0.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.43.0",
    "jsdom": "^9.1.0",
    "nyc": "^10.2.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "rimraf": "^2.5.0",
    "testdouble": "^2.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "highlight.js": "^9.0.0",
    "prop-types": "^15.5.6"
  },
  "nyc": {
    "all": true,
    "require": [
      "babel-register"
    ],
    "reporter": [
      "text",
      "json"
    ],
    "include": [
      "src/**/*.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "ava": {
    "require": [
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit",
    "files": [
      "test/*.js"
    ],
    "source": [
      "src/**/*.js"
    ]
  }
}
